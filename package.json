  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./__tests__/setup.js"
    ],
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/bower_components/"
    ],
    "collectCoverage": false,
    "coverageDirectory": "__coverage__",
    "testRegex": ".*(test|spec).js$",
    "verbose": true,
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|native-base|react-navigation|react-native-fabric|victory|victory-native)"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss|mp4)$": "<rootDir>/__tests__/SCSSStub.js"
    },
    "globals": {
      "window": {
        "setTimeout": true
      }
    }
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "output": "test/junit.xml",
    "classNameTemplate": "{classname} - {title}",
    "titleTemplate": "{classname} - {title}",
    "ancestorSeparator": " > ",
    "usePathForSuiteName": "true"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./app"
      ],
      "pattern": "**/*.mob.stories.js",
      "outputFile": "./storybook/storyLoader.js"
    }
  }
}
